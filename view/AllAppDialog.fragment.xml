<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Products Found" 
		title="Select Colums to Display"
		search="handleSearchTableAllApp"
		confirm="handleCloseTableAllApp"
		close="handleCloseTableAllApp"
		multiSelect="true"
		items="{
			path: '/allapp'
		}" >
		<StandardListItem
			title="{COLUMN_NAME}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>