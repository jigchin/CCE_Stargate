<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Products Found"
		title="Select Colums to Display"
		search="handleSearchTablePastApp"
    		confirm="handleCloseTablePastApp"
		close="handleCloseTablePastApp"
		multiSelect="true"
		items="{
			path: '/pastapp'
		}" >
		<StandardListItem
			title="{COLUMN_NAME}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>