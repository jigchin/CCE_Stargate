<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Products Found"
		title="Select Colums to Display"
		search="handleSearchTableCompApp"
    		confirm="handleCloseTableCompApp"
		close="handleCloseTableCompApp"
		multiSelect="true"
		items="{
			path: '/compapp'
		}" >
		<StandardListItem
			title="{COLUMN_NAME}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>