<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Products Found"
		title="Select Colums to Display"
		search="handleSearchMyTable"
		confirm="handleCloseMyTable"
		close="handleCloseMyTable"
		multiSelect="true"
		items="{
			path: '/mydf'
		}" >
		<StandardListItem
			title="{COLUMN_NAME}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>