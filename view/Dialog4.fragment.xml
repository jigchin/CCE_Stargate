<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
	    contentWidth="1900px"
		noDataText="No Products Found"
		multiSelect="true"
		title="Select Column"
		search="handleSearch1"
		confirm="handleClose4"
		close="handleClose4"
		items="{
			path : '/dbcolumns'
		}">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier text="{COLUMN_NAME}" />
		
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="4em" minScreenWidth="Desktop">
				<header>
					<Text text="Show All Columns" />
				</header>
			</Column>
			
			
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>